{
  "admin": "@",
  "domain": [],
  "adminOnly": true,
  "efconventions": {
    "maxlength": {
      "Title": 50,
      "Description": 500,
      "Image": 300,
      "Url": 300,
      "Key": 50,
      "ContentType": 20,
      "Source": 300,
      "Value": 100,
      "Id": 50
    },
    "removename": [
      "Identity"
    ]
  },
  "CookieAuthentication": {
    "login": "/auth/login",
    "logout": "/auth/logout"
  },
  "antiforgery": {
    "CookieName": "XSRF-TOKEN",
    "HeaderName": "X-XSRF-TOKEN",
    "RequireSsl": true,
    "SuppressXFrameOptionsHeader": false
  },
  "ApplicationInsights": {
    "InstrumentationKey": "*"
  }
}